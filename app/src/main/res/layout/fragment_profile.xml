<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/fotoProfilo"
        android:layout_width="106dp"
        android:layout_height="111dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.107"
        app:srcCompat="@drawable/ic_profile_black_24dp" />

    <TextView
        android:id="@+id/nome"
        android:layout_width="434dp"
        android:layout_height="64dp"
        android:layout_marginStart="175dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="175dp"
        android:layout_marginBottom="526dp"
        android:text=" Nome Cognome"
        android:textAlignment="center"
        android:textColor="#3F51B5"
        android:textSize="38sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fotoProfilo"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/dataNascita"
        android:layout_width="61dp"
        android:layout_height="21dp"
        android:text="dataNascita"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.383"
        tools:textSize="10sp" />

    <TextView
        android:id="@+id/telefono"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="188dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="187dp"
        android:layout_marginBottom="469dp"
        android:text="cellulare"
        android:textColor="#3F51B5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dataNascita"
        app:layout_constraintVertical_bias="0.0"
        tools:textSize="38sp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="108dp"
        android:layout_height="69dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="381dp"
        android:layout_marginEnd="342dp"
        android:layout_marginBottom="323dp"
        android:text="LIVELLO  ARRAMPICATA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.48"
        tools:textSize="50sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="110dp"
        android:layout_height="51dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="1dp"
        android:layout_marginBottom="278dp"
        android:text="PERSONAL BEST"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.079"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.03"
        tools:textSize="50sp" />

</androidx.constraintlayout.widget.ConstraintLayout>